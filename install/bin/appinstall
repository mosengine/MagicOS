echo "Загрузка приложения "$1
echo "___________________________________"

cd /tmp; mkdir magic; cd magic; mkdir apps; cd apps; mkdir $1; cd $1;

wget 'https://raw.githubusercontent.com/mosengine/MagicOS/master/apps/'$1'/download'

cat /tmp/magic/apps/$1/download | while IFS= read -r dld
do
	echo $dld

done

